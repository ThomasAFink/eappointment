{
  "name": "zmsadmin",
  "version": "1.0.0",
  "description": "zms admin frontend ",
  "main": "index.js",
  "private": true,
  "author": "BerlinOnline GmbH & Co. KG",
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint js/",
    "fix": "eslint --fix js/",
    "css": "parcel build scss/admin.scss --log-level verbose --detailed-report --target css",
    "js": "parcel build js/index.js --log-level verbose --detailed-report --target js",
    "csswatch": "parcel watch scss/admin.scss --log-level verbose  --target css",
    "jswatch": "parcel watch js/index.js --log-level verbose --no-hmr --target js",
    "reactwatch": "parcel watch js/reactcomponents.js --log-level verbose --no-hmr --target js",
    "watch": "npm run jswatch & npm run csswatch &"
  },
  "targets": {
    "js": {
      "publicUrl": "./",
      "distDir": "public/_js/"
    },
    "css": {
      "publicUrl": "./",
      "distDir": "public/_css/"
    }
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.6.2",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-react": "^7.24.0",
    "jquery": "^3.6.0",
    "js-cookie": "^2.2.1",
    "json-schema-defaults": "^0.4.0",
    "json-schema-ref-parser": "^9.0.9",
    "moment": "^2.29.1",
    "parcel": "^2.6.2",
    "prop-types": "^15.7.2",
    "redux-json-schema": "^1.0.0",
    "react-datepicker": "^4.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "bo-layout-admin-scss": "https://gitlab.com/eappointment/includes/layout-admin-scss.git",
    "bo-layout-admin-js": "https://gitlab.com/eappointment/includes/layout-admin-js.git",
    "bo-zmsentities": "https://gitlab.com/eappointment/zmsentities.git"
  }
}
