{% block estimatedWaitingTime %}
{% trans "Vorraussichtliche Wartezeit" %}:
{% if waitingTime %}
	{% if waitingTime < 120 %}
        {{ waitingTime }} {% trans "Minuten" %}
    {% else %}
    	{{ (waitingTime / 60)|round(0, 'floor') }} {% trans "Stunden" %}
    {% endif %}
{% else %}
    {% trans "keine" %}
{% endif %}
{% endblock %}
