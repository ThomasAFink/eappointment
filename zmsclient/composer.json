{
    "name": "eappointment/zmsclient",
    "version": "1.0.0",
    "type": "library",
    "description": "Use this library to fetch data from the eappointment API via HTTP.",
    "license": "MIT",
    "authors": [
        {
            "name": "Mathias Fischer",
            "email": "mathias.fischer@berlinonline.de"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../*",
            "options": {
                "symlink": true
            }
        }
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require": {
        "php": ">=7.3.0",
        "ext-curl": ">=0",
        "ext-mbstring": ">=0",
        "ext-json": ">=1.0",
        "ext-pcre": ">=0",
        "eappointment/zmsentities": "*@dev",
        "eappointment/zmsslim": "*@dev",
        "eappointment/mellon": "*@dev",
        "psr/http-message": "~1.1.0",
        "aronduby/dump": "~0.9.0",
        "php-http/curl-client": "~2.3.0",
        "slim/psr7": "~1.7.0",
        "tracy/tracy": "~2.10.7",
        "twig/twig": "~3.10.3"
    },
    "scripts": {
        "command": "bin/configure"
    },
    "autoload": {
        "psr-4": {
            "BO\\Zmsclient\\": "src/Zmsclient/",
            "BO\\Zmsclient\\Tests\\": "tests/Zmsclient/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.6.0",
        "phpspec/prophecy-phpunit": "~2.2.0",
        "phpmd/phpmd": "~2.15.0",
        "squizlabs/php_codesniffer": "~3.10.0",
        "helmich/phpunit-psr7-assert": "~4.4.1"
    }
}