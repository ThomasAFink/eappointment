{
    "type": "object",
    "description": "waiting queue entry with clients",
    "example": {
        "arrivalTime": 1447922893000,
        "callCount": 1,
        "callTime": 1447922938000,
        "number": "123",
        "waitingTime": 2
    },
    "required": [
        "arrivalTime",
        "callCount",
        "callTime",
        "number",
        "waitingTime"
    ],
    "properties": {
        "arrivalTime": {
            "type": "integer",
            "description": "unix timestamp when client was added to the queue or appointment time"
        },
        "callCount": {
            "type": "integer",
            "description": "number of calls for the client to appear at the workstation"
        },
        "callTime": {
            "type": "integer",
            "description": "unix timestamp of last call"
        },
        "number": {
            "type": "string",
            "description": "number to display on a call display"
        },
        "waitingTime": {
            "type": "integer",
            "description": "minutes since arrival of client"
        }
    }
}
