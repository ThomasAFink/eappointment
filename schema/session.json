{
    "type": "object",
    "description": "Representation of a session. It usually stores temporary appointment data before a free appointment is found.",
    "example": {
        "name": "Zmsappointment",
        "id": "7b89b1c1fd6c7a52fa748ff663babd0c",
        "content": {
            "basket": {
                "requests" : "120335",
                "providers" : "122217",
                "scope" : "123",
                "process" : "1234567",
                "date" : "1456310041",
                "familyName" : "Max Mustermann",
                "clientEmail" : "max@zms-bo.de",
                "clientTelehone" : "030 23232323",
                "clientAmendment" : "Notiz zum Termin",
                "authKey" : "1234",
                "notificationConfirmation" : false
            },
            "human": {
                "captcha_text" : "kck6Qz",
                "client" : 0,
                "ts" : 0,
                "origin" : "captcha",
                "remoteAddress" : "127.0.0.1",
                "referrer" : "termin",
                "step" : {
                    "dayselect":1
                }
            },
            "entry" : {
                "source" : "specific",
                "provider" : "122217",
                "request" : "120335"
            },
            "task" : "continue",
            "status" : "free",
            "finished" : true,
            "X-Authkey" : "abc12345678def",
            "error" : ""
        }
    },
    "required" : [
        "id",
        "name"
        ],
    "properties": {
        "name": {
            "type": "string",
             "description": "name of session"
        },
        "id": {
            "type": "string",
             "description": "id of session"
        },
        "content": {
            "type": "object",
            "properties": {
                "basket" : {
                    "type": "object",
                    "properties": {
                        "requests" : {
                            "type": "string"
                        },
                        "providers" : {
                            "type": "string"
                        },
                        "scope" : {
                            "type": ["number", "null"]
                        },
                        "process" : {
                            "type": ["number", "null"]
                        },
                        "date" : {
                            "type": ["number", "null"]
                        },
                        "familyName" : {
                            "type": "string"
                        },
                        "clientEmail" : {
                            "type": "string"
                        },
                        "clientTelehone" : {
                            "type": "number"
                        },
                        "clientAmendment" : {
                            "type": "string"
                        },
                        "authKey" :{
                            "type": "string"
                        },
                        "notificationConfirmation" : {
                        	"type": "boolean"
                        }
                    }
                },
                "human" : {
                    "type" : "object",
                    "properties" : {
                        "captcha_text" : {
                            "type": "string"
                        },
                        "client" : {
                            "type": "number"
                        },
                        "ts" : {
                            "type": "number"
                        },
                        "origin" : {
                            "type": "string"
                        },
                        "remoteAddress" : {
                            "type": "string"
                        },
                        "referrer" : {
                            "type": "string"
                        },
                        "step" : {
                            "type": "object",
                            "description": "amount of called booking steps",
                            "properties": {
                                "dayselect": {
                                    "type": "number"
                                },
                                "timeselect": {
                                    "type": "number"
                                },
                                "register": {
                                    "type": "number"
                                },
                                "confirm": {
                                    "type": "number"
                                }
                            }
                        }
                    }
                },
                "entry" : {
                    "type" : "object",
                    "description": "Save request IDs and provider IDs from entry query",
                    "properties": {
                        "source" : {
                            "type": "string"
                        },
                        "provider" : {
                            "type": "string"
                        },
                        "request" :{
                            "type": "string"
                        }
                    }
                },
                "status" : {
                    "type": "string",
                    "enum": [
		                "start",
		                "reserved",
		                "confirmed",
		                "inChange",
		                "changedProcess",
		                "stalled",
		                "inProgress"
		            ],
            		"description": "Status of the session. The following values are possible:\n* start: new session started\n* reserved: set if a process has been reserved\n* confirmed: set if a process has been confirmed\n* inChange: current process is in change mode\n* changedProcess: current process has been changed\n* stalled: set if session has a problem, fore example if second tab is in use \n* inProgress: if session not stalled anymore\n"
                },
                "task" : {
                    "type": "string",
                    "enum": [
                    	"new",
		                "continue"
			            ],
	            		"description": "Change normal process by a task: \n* new: force to delete reserved process if exists, remove last step and start modifying process\n* continue: exception to continue on breaking status like reserved or confirmed\n"
                },
                "finished" : {
                    "type": "boolean",
                    "description": "True if process has been confirmed successfully"
                },
                "X-Authkey" : {
                    "type": "string"
                },
                "error" : {
                    "type": "string",
                    "description": "error or notice from errorhandler to show specific template message block"
                }
            }
        }
    }
}
