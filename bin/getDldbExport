#!/bin/bash
REALPATH=$(which realpath || which readlink)
REALPATH=$([[ "$REALPATH" =~ 'readlink' ]] && echo "$REALPATH -e" || echo "$REALPATH")
DIR=$(dirname $($REALPATH ${BASH_SOURCE[0]}))
ROOT=$($REALPATH $DIR/..)

EXPORT=$ROOT/data/
PROXY=proxy:3128

$ROOT/vendor/bo/clientdldb/bin/dldbget -o $EXPORT -p $PROXY