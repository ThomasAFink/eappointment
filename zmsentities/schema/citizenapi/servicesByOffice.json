{
  "type": "object",
  "description": "Lists services associated with specific offices.",
  "properties": {
    "services": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Unique identifier for the service",
            "pattern": "^[A-Za-z0-9_-]+$",
            "minLength": 1
          },
          "name": {
            "type": "string",
            "description": "Display name of the service",
            "minLength": 1
          },
          "maxQuantity": {
            "type": "number",
            "description": "Maximum number of appointments that can be booked for this service",
            "minimum": 1
          }
        },
        "required": [
          "id",
          "name",
          "maxQuantity"
        ]
      }
    }
  },
  "required": [
    "services"
  ]
}
