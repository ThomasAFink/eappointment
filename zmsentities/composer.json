{
    "name": "eappointment/zmsentities",
    "version": "1.0.0",
    "type": "library",
    "description": "Entity definitions for eappoinment",
    "license": "MIT",
    "authors": [
        {
            "name": "Mathias Fischer",
            "email": "mathias.fischer@berlinonline.de"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "ext-curl": ">=0",
        "ext-mbstring": ">=0",
        "ext-json": ">=1.0",
        "ext-pcre": ">=0",
        "eappointment/mellon": "*@dev",
        "twig/twig": "~3.10.0",
        "league/json-guard": "~1.0.0",
        "league/html-to-markdown": "~5.1.0",
        "giggsey/libphonenumber-for-php": "~8.13.0",
        "twig/intl-extra": "~3.10.0",
        "symfony/translation": "~5.4.40",
        "symfony/twig-bridge": "~5.4.40"
    },
    "scripts": {
        "command": "bin/configure"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../*",
            "options": {
                "symlink": true
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "BO\\Zmsentities\\": "src/Zmsentities/",
            "BO\\Zmsentities\\Tests\\": "tests/Zmsentities/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.6.0",
        "helmich/phpunit-psr7-assert": "~4.4.1",
        "phpmd/phpmd": "~2.15.0",
        "squizlabs/php_codesniffer": "~3.7.0",
        "league/json-reference": "~1.1.0",
        "yuloh/json-guard-cli": "~0.3.0",
        "roave/security-advisories": "dev-latest"
    }
}